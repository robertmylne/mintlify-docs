        {
          "anchor": "Website",
          "href": "https://caspen.com",
          "icon": "browser"
        }
      ]
    },
    "groups": [
      {
        "group": "API documentation",
        "pages": [
          "introduction",
          "authentication",
          "errors",
          "pagination",
          "security",
          "rate-limits",
