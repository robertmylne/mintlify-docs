{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "Caspen API docs",
  "api": {
    "playground": {
      "display": "simple"
    }
  },
  "colors": {
    "primary": "#4338CA",
    "light": "#818cf8",
    "dark": "#3730A3"
  },
  "favicon": "/favicon.svg",
